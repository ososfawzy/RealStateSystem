  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <SQLQuery><![CDATA[SELECT xrc.*,  
       rep.property_name,  
       (SELECT DISTINCT ript.installment_payment_term_id  
          FROM xxma_rs_install_payment_terms ript  
         WHERE ript.installment_payment_term_id =  
                  (SELECT DISTINCT installment_payment_term_id  
                     FROM xxma_rs_contract_prop_payment rcpp,  
                          xxma_rs_install_term_details ritd  
                    WHERE     ritd.installment_term_detail_id =  
                                 rcpp.installment_term_detail_id  
                          AND rcpp.contract_id = xrc.contract_id))  
          installment_payment_term_id  
  FROM xxma_rs_contract xrc, xxma_real_estate_property rep  
 WHERE xrc.ext_attribute10 = rep.property_id]]></SQLQuery>
