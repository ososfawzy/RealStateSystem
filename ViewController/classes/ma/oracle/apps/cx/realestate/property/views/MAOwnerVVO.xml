<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="MAOwnerVVO"
  Version="11.1.1.65.57"
  InheritPersonalization="true"
  BindingStyle="Oracle"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
