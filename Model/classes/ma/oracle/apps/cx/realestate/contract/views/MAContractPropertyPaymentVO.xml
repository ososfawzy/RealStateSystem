  <SQLQuery><![CDATA[SELECT marscontractproppaymenteo.amount,  
       marscontractproppaymenteo.contract_id,  
       marscontractproppaymenteo.contract_property_id,  
       marscontractproppaymenteo.created_by,  
       marscontractproppaymenteo.creation_date,  
       marscontractproppaymenteo.due,  
       marscontractproppaymenteo.due_date,  
       marscontractproppaymenteo.ext_attribute1,  
       marscontractproppaymenteo.ext_attribute10,  
       marscontractproppaymenteo.ext_attribute11,  
       marscontractproppaymenteo.ext_attribute12,  
       marscontractproppaymenteo.ext_attribute13,  
       marscontractproppaymenteo.ext_attribute14,  
       marscontractproppaymenteo.ext_attribute15,  
       marscontractproppaymenteo.ext_attribute2,  
       marscontractproppaymenteo.ext_attribute3,  
       marscontractproppaymenteo.ext_attribute4,  
       marscontractproppaymenteo.ext_attribute5,  
       marscontractproppaymenteo.ext_attribute6,  
       marscontractproppaymenteo.ext_attribute7,  
       marscontractproppaymenteo.ext_attribute8,  
       marscontractproppaymenteo.ext_attribute9,  
       marscontractproppaymenteo.ext_attribute_category,  
       marscontractproppaymenteo.installment_number,  
       marscontractproppaymenteo.installment_term_detail_id,  
       marscontractproppaymenteo.int_attribute1,  
       marscontractproppaymenteo.int_attribute10,  
       marscontractproppaymenteo.int_attribute11,  
       marscontractproppaymenteo.int_attribute12,  
       marscontractproppaymenteo.int_attribute13,  
       marscontractproppaymenteo.int_attribute14,  
       marscontractproppaymenteo.int_attribute15,  
       marscontractproppaymenteo.int_attribute2,  
       marscontractproppaymenteo.int_attribute3,  
       marscontractproppaymenteo.int_attribute4,  
       marscontractproppaymenteo.int_attribute5,  
       marscontractproppaymenteo.int_attribute6,  
       marscontractproppaymenteo.int_attribute7,  
       marscontractproppaymenteo.int_attribute8,  
       marscontractproppaymenteo.int_attribute9,  
       marscontractproppaymenteo.int_attribute_category,  
       marscontractproppaymenteo.intrest,  
       marscontractproppaymenteo.last_update_date,  
       marscontractproppaymenteo.last_update_login,  
       marscontractproppaymenteo.last_updated_by,  
       marscontractproppaymenteo.maturity_date,  
       marscontractproppaymenteo.notes,  
       marscontractproppaymenteo.paid,  
       marscontractproppaymenteo.paid_on,  
       marscontractproppaymenteo.payment_date,  
       marscontractproppaymenteo.payment_method,  
       marscontractproppaymenteo.payment_number,  
       marscontractproppaymenteo.property_id,  
       marscontractproppaymenteo.receipt_number,  
       marscontractproppaymenteo.rent_term_detail_id,  
       prop.property_code,  
       cont.contract_number,  
       rco.owner_name,  
       itd.payment_type,  
       DECODE (  
          (SELECT maturity_date  
             FROM xxma_rs_contract_prop_payment  
            WHERE     contract_property_id =  
                         marscontractproppaymenteo.contract_property_id  
                  AND maturity_date <= SYSDATE),  
          NULL, 'Not Due',  
          (DECODE (  
              (SELECT amount  
                 FROM xxma_rs_contract_prop_payment  
                WHERE     contract_property_id =  
                             marscontractproppaymenteo.contract_property_id  
                      AND NVL (paid, 0) < amount),  
              NULL, 'Full Paid',  
              (DECODE (  
                  (SELECT amount  
                     FROM xxma_rs_contract_prop_payment  
                    WHERE     contract_property_id =  
                                 marscontractproppaymenteo.contract_property_id  
                          AND NVL (paid, 0) < amount  
                          AND paid IS NOT NULL),  
                  NULL, 'Due and not paid',  
                  'Partially Paid')))))  
          status  
  FROM xxma_rs_contract_prop_payment marscontractproppaymenteo,  
       xxma_rs_contract cont,  
       xxma_rs_contract_owner rco,  
       xxma_real_estate_property prop,  
       xxma_rs_install_term_details itd  
 WHERE     marscontractproppaymenteo.contract_id = cont.contract_id  
       AND cont.contract_id = rco.contract_id  
       AND rco.int_attribute3 = 'Y'  
       AND cont.ext_attribute10 = prop.property_id  
       AND itd.installment_term_detail_id =  
              marscontractproppaymenteo.installment_term_detail_id]]></SQLQuery>
