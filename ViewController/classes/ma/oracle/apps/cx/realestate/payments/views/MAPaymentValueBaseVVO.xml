  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <SQLQuery><![CDATA[SELECT xlv.lookup_code, xlv.meaning value_base, xlt.lookup_type  
  FROM xxma_lookup_types xlt, xxma_lookup_values xlv  
 WHERE     xlt.lookup_id = xlv.lookup_id  
       AND xlt.lookup_type = 'MSH_PYMENT_VL_BASE_LKP']]></SQLQuery>
