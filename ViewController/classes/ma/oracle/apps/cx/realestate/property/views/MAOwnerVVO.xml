  </Properties>
  <SQLQuery>
    <![CDATA[select PERSON_ID owner_id,LAST_NAME full_name
from
XXMA.PER_ALL_PEOPLE_F
WHERE PERSON_ID BETWEEN 1 AND 20
AND SYSDATE BETWEEN EFFECTIVE_START_DATE AND EFFECTIVE_END_DATE]]>
  </SQLQuery>
  <ViewAttribute
